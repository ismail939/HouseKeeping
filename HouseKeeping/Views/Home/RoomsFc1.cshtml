@using Microsoft.VisualBasic


<div class="container">

@{
    var rooms = new List<string> { "101", "102", "103", "104", "101", "102", "103", "104", "105", "106", "107", "108", "109", "110", "101", "102", "103", "104" };
}
    <div class="d-flex flex-wrap">
        @foreach (var room in rooms)
        {
            
            Console.WriteLine($"Processing room: {room}");
            
            <div class="p-2" style="flex: 0 0 20%; max-width: 20%;">
                    <a href="#" class="card-link">
                        <div class="card text-white bg-primary shadow h-100">
                            <div class="card-body">
                                <h3 class="card-title">@room</h3>
                            </div>
                        </div>
                    </a>
            </div>
        }
    </div>
</div>